{
  "name": "v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "build:staging": "tsc -b && vite build --mode staging",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "deploy:gcp": "npm run build:prod && gsutil -m rsync -r -d dist/ gs://your-production-bucket/",
    "deploy:firebase": "npm run build:prod && firebase deploy",
    "deploy:netlify": "npm run build:prod && netlify deploy --prod --dir=dist",
    "test:build": "npm run build:prod && npm run preview:prod",
    "analyze": "npm run build:prod && npx vite-bundle-analyzer dist/assets/*.js"
  },
  "dependencies": {
    "aceternity-ui": "^0.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.46",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-uniui": "^1.3.2",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/google.maps": "^3.58.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
